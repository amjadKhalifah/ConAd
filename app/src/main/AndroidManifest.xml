<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="conad.tum.conad" >

    <uses-permission android:name="com.google.android.glass.permission.DEVELOPMENT" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SplashActivity"
            android:icon="@drawable/ic_glass_logo"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.google.android.glass.action.VOICE_TRIGGER" />
            </intent-filter>

            <meta-data
                android:name="com.google.android.glass.VoiceTrigger"
                android:resource="@xml/voice_trigger" />
        </activity>
        <activity
            android:name=".TestStartupActivity"
            android:icon="@drawable/ic_glass_logo"
            android:label="@string/title_activity_response_time_test1" >
        </activity>
        <activity
            android:name=".ColorResponseTimeTestActivity"
            android:label="@string/title_activity_color_response_time_test" >
        </activity>
        <activity
            android:name=".ResponseResultActivity"
            android:icon="@drawable/ic_glass_logo"
            android:label="@string/title_activity_response_result" >
        </activity>
        <activity
            android:name=".MemoryTestActivity"
            android:label="@string/title_activity_memory_test" >
        </activity>
        <activity
            android:name=".MemoryTestStartupActivity"
            android:label="@string/title_activity_memory_test" >
        </activity>
        <activity
            android:name=".AwarenessTestStartupActivity"
            android:label="@string/title_activity_memory_test" >
        </activity>
        <activity
            android:name=".AwarenessTestActivity"
            android:label="@string/title_activity_awareness_test" >
        </activity>
        <activity
            android:name=".MainResultActivity"
            android:icon="@drawable/ic_glass_logo"
            android:label="@string/title_activity_main_result" >
        </activity>
    </application>

</manifest>
